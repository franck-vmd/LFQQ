<?xml version="1.0" encoding="utf-8"?>
<PropertyList>

  <path>laker-csl.ac</path>

  <effect>
    <inherits-from>/Effects/model-combined-deferred</inherits-from>
    <parameters>
      <lightmap-enabled type="int">1</lightmap-enabled>
      <lightmap-factor><use>/environment/lightmap-factor</use></lightmap-factor>
      <transparent>true</transparent>
      <texture n="3">
        <image>laker2.png</image>
        <wrap-s>repeat</wrap-s>
      </texture>
    </parameters>
    <object-name>Crew quarters</object-name>
    <object-name>Hull</object-name>
    <object-name>Navigation lights</object-name>
    <object-name>Stack CSL</object-name>
    <object-name>Wheelhouse</object-name>
  </effect>

  <!-- hide objects not in this variant -->
  <animation>
    <type>select</type>
    <object-name>Bulk loads</object-name>
    <object-name>Deck crane</object-name>
    <object-name>Stack CTC</object-name>
    <condition>
      <false/>
    </condition>
  </animation>
  
</PropertyList>
