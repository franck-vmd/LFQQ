<PropertyList>
	<path>jetway-737-ba.ac</path>

	<animation>
		<type>material</type>
		<object-name>Mesh</object-name>
		<condition>
			<greater-than>
				<property>/sim/time/sun-angle-rad</property>
				<value>1.57</value>
			</greater-than>
		</condition>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
		</emission>
		<texture>jetway-737-ba_lit.png</texture>
	</animation>
	
	<animation>
		<type>material</type>
		<object-name>Mesh</object-name>
		<condition>
			<less-than-equals>
				<property>/sim/time/sun-angle-rad</property>
				<value>1.57</value>
			</less-than-equals>
		</condition>
		<emission>
			<red>0</red>
			<green>0</green>
			<blue>0</blue>
		</emission>
		<texture>jetway-737-ba.png</texture>
	</animation>

</PropertyList>