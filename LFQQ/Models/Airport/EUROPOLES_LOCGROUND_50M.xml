<PropertyList>

<path>EUROPOLES_LOCGROUND_50M.ac</path>

<animation>
 <type>material</type>
    <object-name>locground_redlight</object-name>
 <condition>		
  <greater-than>		
   <property>/sim/time/sun-angle-rad</property>
   <value>1.57</value>
  </greater-than>
 </condition>
 <emission>	
  <red>1.0</red>	
  <green>1.0</green>	
  <blue>1.0</blue>
 </emission>
</animation>

</PropertyList>